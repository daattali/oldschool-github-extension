{
  "name": "Old School GitHub",
  "version": "1.2",
  "manifest_version": 2,
  "description": "GitHub changed their UI on 2020-06-23; this extension reverts many components back to their classic look.",
  "icons": {
    "16": "img/icon-16.png",
    "48": "img/icon-48.png",
    "128": "img/icon-128.png"
  },
  "content_scripts": [
    {
      "matches": [
        "https://github.com/*",
        "https://gist.gihub.com/*"
      ],
      "css": ["css/osgh.css"],
      "run_at": "document_start"
    }
  ],
  "homepage_url": "https://github.com/daattali/oldschool-github-extension"
}
